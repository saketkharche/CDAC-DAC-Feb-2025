#include <iostream>
#include <string.h>
using namespace std;
class A
{
public:
int Num1;
void PrintDataA()
{
	cout<<"Num1: "<<this->Num1<<endl;
}
};
class B: virtual public A
{
public:
int Num2;
B()
{
	this->Num1=10;
	this->Num2=20;
}
void PrintDataB()
{
	cout<<"Num1: "<<this->Num1<<endl;
	cout<<"Num2: "<<this->Num2<<endl;
}
};

int main()
{

	return 0;
}
