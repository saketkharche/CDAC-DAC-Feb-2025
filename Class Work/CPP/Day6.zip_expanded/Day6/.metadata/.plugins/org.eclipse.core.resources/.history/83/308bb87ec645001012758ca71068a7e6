#include <iostream>
#include <string.h>
using namespace std;
class A
{
private:
	int Num1;
public:

	int Num2;

	int getNum1() const {
		return Num1;
	}

	void setNum1(int num1) {
		Num1 = num1;
	}
};
class B:public A
{
public:
	void GetData()
	{
		this->setNum1(100);
		this->Num2=200;
	}
};
int main()
{
	B b1;
	b1.GetData();

	cout<<"Num1 of b1: "<<b1.getNum1()<<endl;  //NOT OK, because Num1 becomes private in class A and inheritted as private in B
	cout<<"Num2 of b1: "<<b1.Num2<<endl;

	return 0;
}
