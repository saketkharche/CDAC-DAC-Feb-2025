#include <iostream>
using namespace std;

struct Employee
{
	unsigned int EmpId;
	//char Name[50];
	float Salary;
}e1,e2,e3,e4;

void PrintEmployee(Employee e5)		//Global Method printing values of Employee object
{
	printf("EmpId: %d", e5.EmpId);
	//printf("Name: %s", e5.Name);
	printf("\nSalary: %f", e5.Salary);
}
int main()
{

	struct Employee e5,e6,e7;

	e5.EmpId=1001;
	//e5.Name="Malkeet";
	e5.Salary=56778.89;

	PrintEmployee(e5);


	return 0;
}



