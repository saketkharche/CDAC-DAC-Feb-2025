#include <iostream>
using namespace std;

//Datatypes
//Arrays
//struct

struct Student {
	string name;
	int age;
	float gpa;
	void AcceptStudentData()
	{
		Student s;
		s.name = "John";
		s.age = 20;
		s.gpa = 3.5;
		printf("Name: %s, Age: %d, GPA: %.2f\n", s.name.c_str(), s.age, s.gpa);
	}
}s1,s2,s3;


int main() {

	s1.AcceptStudentData();

	return 0;
}

